package sec01.ex01;

public class MemberVO {
	
	String id;
	String pwd;
	String name;
	String email;
	
	public MemberVO() {
		System.out.println("MemberVO 생성자 호출");
	}
	
	public MemberVO(String id, String pwd, String name, String email) { //인자 4개를 갖는 생성자
		this.id = id;
		this.pwd = pwd;
		this.name = name;
		this.email = email;
	}
	
	public MemberVO(String id, String pwd, String name, String email, Date joinDate) { //인자 3개를 갖는 생성자
		this.id = id;
		this.pwd = pwd;
		this.name = name;
		this.email = email;
		this.joinDate = joinDate;
	}
	
	//각 속성에 대한 getter/setter
	

}
